  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["lim", 
      RowBox[{"x", " ", "\[Rule]", "\[VeryThinSpace] ", "1"}]], " ", 
     SuperscriptBox["x", 
      RowBox[{"1", "/", "3"}]]}], "=", "1"}], TraditionalForm]],
  FontSize->18,ExpressionUUID->"0da55172-af02-485a-bf47-b51c04a0e312"],
